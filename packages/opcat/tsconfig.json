    "allowJs": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "types",
    "strict": false,
    "resolveJsonModule": true
  },
  "include": ["cjs/**/*.cjs"],
  "exclude": ["node_modules/**/*"]
}